  <owl:Class rdf:about="http://www.semanticweb.org/jeffery/ontologies/2021/5/hydro-ontology.owl#上升流">
    <rdfs:label>涌升流</rdfs:label>
    <dc:source>Ontology Engineer ingGroup</dc:source>
    <dc:creator>zjyan</dc:creator>
    <rdfs:subClassOf>
